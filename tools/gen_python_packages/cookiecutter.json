{
    "project_name": "bentoml_monitoring_providers",
    "project_description": "BentoML Monitoring Providers",
    "project": "{{ cookiecutter.project_name | lower | replace('_','-') }}",
    "folder": "{{ cookiecutter.project | splitpart('-',0) }}",
    "features": "{{ cookiecutter.project | splitpart('-',1) }}",
    "provider": "{{ cookiecutter.project | splitpart('-',2) }}",
    "bentoml_version": "{{ cookiecutter.bentoml_version }}",
    "year": "{% now 'utc', '%Y' %}",
    "author": "{{ cookiecutter.author }}",
    "_extensions": ["bootstrap.splitpart","jinja2_time.TimeExtension"]
}
